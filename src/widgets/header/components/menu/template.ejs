<%
const {
	items = [],
} = { ...arguments?.[0] }
%>

<nav class="header__menu">
	<ul class="header__menu-list">
		<% items.forEach(({ href, label }) => { %>
			<li class="header__menu-item">
				<a class="header__menu-link" href="<%= href %>">
					<%= label %>
				</a>
			</li>
		<% }) %>
	</ul>
</nav>