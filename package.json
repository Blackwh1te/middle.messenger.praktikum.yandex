{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "repository": "git@github.com:Blackwh1te/middle.messenger.praktikum.yandex.git",
  "author": "a.lamkov <a.lamkov@vk.team>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "start": "yarn build && yarn serve",
    "start-watch": "concurrently \"yarn serve\" \"sleep 3 && yarn run watch\" & open http://localhost:3000",
    "dev": "parcel 'static/*.html' --open",
    "build": "parcel build 'static/*.html'",
    "watch": "parcel watch 'static/*.html'",
    "serve": "pkill node & node server.js"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "concurrently": "7.6.0",
    "events": "^3.3.0",
    "express": "4.18.2",
    "parcel": "2.8.3",
    "path-browserify": "1.0.0",
    "postcss": "8.4.21",
    "postcss-custom-media": "9.1.2",
    "postcss-easy-import": "4.0.0",
    "postcss-for": "2.1.1",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "6.0.1",
    "process": "0.11.10"
  },
  "dependencies": {
    "@babel/preset-react": "7.18.6"
  },
  "alias": {
    "jsxToDOM": "./src/shared/utils/jsxToDOM"
  }
}
